console.log("test")
function toggle(){
	var is_visible = false;
    var input = document.getElementsByClassName("password");
    
    if(is_visible)
    {
        input.type = 'password';
        is_visible = false; 
    }
    else
    {
        input.type = 'text';
        is_visible = true;
    }
}